rm(list=ls(all=TRUE))
setRepositories()
ap <- available.packages()
View(ap)
install.packages("FactoMineR")
install.packages("FactoMineR", dependencies=TRUE, repos='http://cran.rstudio.com/')
install.packages("car", dependencies=TRUE, repos='http://cran.rstudio.com/')
###########################################################
# Clear all environment variables
###########################################################
# Remove previous objects
rm(list=ls(all=TRUE))
######################################
# Load libraries
######################################
library(MASS)
library(lmtest)
library(nortest)
library(Rcmdr)
library(FactoMineR)
######################################
# Generate three samples of normal distribution
######################################
norm1=data.frame(x1=rnorm(200, mean=0, sd=1),x2="norm1-sd=1")
norm2=data.frame(x1=rnorm(200, mean=10, sd=5),x2="norm2-sd=5")
norm3=data.frame(x1=rnorm(200, mean=50, sd=10),x2="norm3-sd=10")
######################################
# Merge Distributions
######################################
tbl=mergeRows(norm1,norm2,common.only = FALSE)
tbl=mergeRows(as.data.frame(tbl),norm3,common.only = FALSE)
######################################
# Distribution summary
######################################
summary(tbl)
with(tbl, Hist(x1,groups=x2,scale="frequency",
breaks="Sturges",col="red"))
Boxplot(x1~x2, data=tbl, id.method="y")
AnovaModel.1 <- aov(x1~x2, data=tbl)
summary(AnovaModel.1)
with(tbl, numSummary(x1, groups=x2, statistics=c("mean", "sd")))
shapiro.test(residuals(AnovaModel.1))
norm1=data.frame(x1=rnorm(200, mean=0, sd=1),x2="norm1-sd=1")
norm2=data.frame(x1=rnorm(200, mean=10, sd=5),x2="norm2-sd=5")
Boxplot(x1~x2, data=tbl, id.method="y")
with(tbl, Hist(x1,groups=x2,scale="frequency",
breaks="Sturges",col="red"))
